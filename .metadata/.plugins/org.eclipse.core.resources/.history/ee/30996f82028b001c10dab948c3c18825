package com.projonboarding.questiontwo;

import java.util.ArrayList;

public class SquarePatch {

	public static void patch(int n) {
		int[][] matrix = new int[n][n];
		
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				matrix[i][j] = n;
			}
		}
		
		ArrayList<Integer> matrixInts = new ArrayList<Integer>();
		
		for (int k = 0; k < n; k++) {
			for (int l = 0; l < n; l++) {
				System.out.print(matrix[k][l]);
				matrixInts.add(matrix[k][l]);
			}
		}
		
		System.out.println();
		
//		System.out.println("[");
//		for (int k = 0; k < n; k++) {
//			System.out.print("[");
//			for (int l = 0; l < n; l++) {
//				System.out.print(matrix[k][l]);
//			}
//			System.out.println("],");
//		}
//		System.out.println("]");
	}
}